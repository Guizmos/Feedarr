{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "App": {
    "DataDir": "data",
    "DbFileName": "feedarr.db",
    "SyncIntervalMinutes": 60,
    "RssLimit": 100,
    "RssLimitPerCategory": 50,
    "RssLimitGlobalPerSource": 250,
    "Updates": {
      "Enabled": true,
      "RepoOwner": "Guizmos",
      "RepoName": "Feedarr",
      "CheckIntervalHours": 6,
      "TimeoutSeconds": 10,
      "AllowPrerelease": false,
      "GitHubApiBaseUrl": "https://api.github.com",
      "GitHubToken": ""
    },
    "ReverseProxy": {
      "TrustedProxies": [
        "127.0.0.1",
        "::1"
      ],
      "TrustedNetworks": []
    }
  }
}
